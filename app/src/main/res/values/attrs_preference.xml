<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChromeTabPreference">
        <attr name="customUrl" format="string" />
        <attr name="useChromeTab" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EggPreference">
        <attr name="android:targetClass" format="string" />
        <attr name="supportAdaptiveIcon" format="enum">
            <enum name="corners" value="1" />
            <enum name="oval" value="2" />
        </attr>
        <attr name="iconRadius" format="dimension" />
        <attr name="iconPadding" format="dimension" />
    </declare-styleable>
</resources>